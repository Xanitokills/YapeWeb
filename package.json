{
  "name": "pws-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack -H 0.0.0.0 -p 3005",
    "start:ad": "cd activeDirectory/ && uvicorn conexion_py:app --reload --port 8000",
    "dev:all": "concurrently \"npm run dev\" \"cmd /c npm run start:ad\"",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p 3002",
    "start:all": "concurrently \"npm run start\" \"cmd /c npm run start:ad\"",
    "lint": "next lint",
    "check": "pnpm lint && pnpm tsc --noEmit",
    "check-planned-end-dates": "tsc -p src/jobs/tsconfig.service.json && node dist/src/jobs/checkPlannedEndDates.js"
  },
