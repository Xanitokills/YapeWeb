    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.461.0",
    "mssql": "^11.0.1",
    "next": "15.0.3",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.9.16",
    "react": "^18.3.1",
    "react-date-range": "^2.0.1",
    "react-day-picker": "^9.3.2",
    "react-dom": "^18.3.1",
